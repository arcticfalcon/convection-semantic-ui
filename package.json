{
  "name": "convection-semantic-ui",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es",
    "prebuild:commonjs": "rimraf dist/commonjs",
    "build:commonjs": "cross-env NODE_ENV=build babel src -d dist/commonjs",
    "prebuild:es": "rimraf dist/es",
    "build:es": "cross-env NODE_ENV=build-es babel src -d dist/es",
    "lint": "eslint ./src/**/**.{js,jsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "pretty": "node_modules/.bin/prettier src/**/*.{js,jsx} --write"
  },
  "author": "Juan Falc√≥n <jcfalcon@gmail.com>",
  "license": "MIT",
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "dependencies": {
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "segmentize": "^0.4.1",
    "semantic-ui-react": "^0.77"
  },
  "peerDependencies": {
    "convection": "^0.0.1",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "^2.5.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "7.2.0",
    "husky": "0.14.3",
    "jest": "21.1.0",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2",
    "prettier": "^1.7.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-styleguidist": "^6.0.24",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arcticfalcon/convection-semantic-ui.git"
  }
}
